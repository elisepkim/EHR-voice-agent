

  {
    "version": 2,
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python"
      },
      {
        "src": "frontend/*.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "api/index.py" },
      { "src": "/(.*)", "dest": "frontend/$1" },
      { "src": "/", "dest": "frontend/live_mastra_demo.html" }
    ]
  }